+  base::FilePath path;
+  if (!data.ReadPath(&path))
+    return false;
+
+  if (path.BaseName() != path)
+    return false;
+
+  auto maybe_basename = base::SafeBaseName::Create(path);
+  if (!maybe_basename)
+    return false;
+
+  *out = *maybe_basename;
+
+  return true;
+}

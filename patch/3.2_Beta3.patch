+
+  wait_for_new_tab.Wait();
+
+  EXPECT_EQ(2, browser()->tab_strip_model()->count());
+
+  base::RunLoop loop;
+  SimulateDragFromOmniboxToWebContents(loop.QuitClosure());
+  loop.Run();
+

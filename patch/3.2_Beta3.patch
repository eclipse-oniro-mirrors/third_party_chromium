+
+  int active_buffer_index_;
+
+  int bytes_per_frame_;
+
+  size_t buffer_size_bytes_;
+
+  SampleFormat sample_format_;
+
+  std::unique_ptr<AudioRendererAdapter> audio_renderer_;
+
+  DISALLOW_COPY_AND_ASSIGN(OHOSAudioOutputStream);
+};
+

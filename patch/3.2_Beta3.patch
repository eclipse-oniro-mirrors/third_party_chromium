+  if (IsFormatAvailable(ClipboardFormatType::GetPlainTextType(), buffer,
+                        data_dst))
+    types->push_back(
+        base::UTF8ToUTF16(ClipboardFormatType::GetPlainTextType().GetName()));
+  if (IsFormatAvailable(ClipboardFormatType::GetHtmlType(), buffer, data_dst))
+    types->push_back(

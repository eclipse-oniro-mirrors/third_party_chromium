+void NWebDelegate::Zoom(float zoomFactor) const {
+  LOG(INFO) << "NWebDelegate::Zoom";
+  if (GetBrowser().get()) {
+    double curFactor = GetBrowser()->GetHost()->GetZoomLevel();
+    GetBrowser()->GetHost()->SetZoomLevel(zoomFactor + curFactor);
+  }

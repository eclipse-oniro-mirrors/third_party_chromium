+  LOG(INFO) << "NWebDelegate::ExecuteJavaScript";
+  if (GetBrowser().get()) {
+    GetBrowser()->GetMainFrame()->ExecuteJavaScript(
+        code, GetBrowser()->GetMainFrame()->GetURL(), 0);
+  }
+}

+///
+// Class used to report permission request for the specified origin.
+///
+/*--cef(source=library)--*/
+class CefAccessRequest : public virtual CefBaseRefCounted {
+ public:
+  ///
+  // Get the origin that is trying to acess the resource.
+  ///
+  /*--cef()--*/

+  COLOR_TYPE_UNKNOWN = -1,
+
+  // RGBA with 8 bits per pixel (32bits total).
+  COLOR_TYPE_RGBA_8888 = 0,
+
+  // BGRA with 8 bits per pixel (32bits total).
+  COLOR_TYPE_BGRA_8888 = 1,
+};

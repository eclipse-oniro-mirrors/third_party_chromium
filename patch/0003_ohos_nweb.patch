+bool OhGinJavascriptBridgeValueConverter::FromV8Object(
+    v8::Local<v8::Object> value,
+    std::unique_ptr<base::Value>* out,
+    v8::Isolate* isolate) {
+  OhGinJavascriptBridgeObject* unwrapped;
+  if (!gin::ConvertFromV8(isolate, value, &unwrapped)) {

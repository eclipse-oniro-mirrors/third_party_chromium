+  {CM_EDITFLAG_CAN_TRANSLATE, CmEf::CM_ES_CAN_TRANSLATE},
+};
+
+const std::unordered_map<int, int> kQmEditStateFlagsMap = {
+  {QM_EDITFLAG_NONE, QmEf::QM_EF_NONE},

+        CM_ES_CAN_PASTE = 1 << 4,
+        CM_ES_CAN_DELETE = 1 << 5,
+        CM_ES_CAN_SELECT_ALL = 1 << 6,
+        CM_ES_CAN_TRANSLATE = 1 << 7,
+    };

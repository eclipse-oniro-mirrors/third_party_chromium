+
+int32_t ConvertMenuFlags(int32_t value,
+  const std::unordered_map<int, int> &flags_map) {
+  int32_t result = 0;
+  for (auto& kv : flags_map) {
+    if ((kv.first & value) != 0) {
+      result |= kv.second;
+    }
+  }
+  return result;
